{
    "name": "northwestern-sysdev/event-hub-php-sdk",
    "description": "PHP SDK for the Northwestern EventHub",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "northwestern",
        "northwestern university"
    ],
    "authors": [
        {
            "name": "Nicholas Evans",
            "email": "nick.evans@northwestern.edu"
        }
    ],
    "require": {
        "php": ">=8.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "php-coveralls/php-coveralls": "^2.4",
        "spatie/invade": "^2.0",
        "laravel/pint": "^1.13",
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "Northwestern\\SysDev\\SOA\\EventHub\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Northwestern\\SysDev\\SOA\\EventHub\\Tests\\": "tests/"
        }
    }
}
